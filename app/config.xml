<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.mendiolaza.radio" version="1.1.143" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Radio Mendiolaza</name>
    <description>
        Escuchar Radio Mendiolaza vía streaming
    </description>
    <author email="radio@data99.com.ar" href="http://radio.mendiolaza.com.ar">
        Equipo técnico de Radio Mendiolaza
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />

    <platform name="android">    
        <icon src="/www/res/logo-64x64.png" density="ldpi" qualifier="ldpi" />
        <icon src="/www/res/logo-64x64.png" density="mdpi" qualifier="mdpi" />
        <icon src="/www/res/logo-64x64.png" density="hdpi" qualifier="hdpi" />
        <icon src="/www/res/logo-64x64.png" density="xhdpi" qualifier="xhdpi" />

        <splash src="www/res/splash.png" density="land-ldpi" qualifier="land-ldpi" />
        <splash src="www/res/splash.png" density="land-mdpi" qualifier="land-mdpi" />
        <splash src="www/res/splash.png" density="land-hdpi" qualifier="land-hdpi" />
        <splash src="www/res/splash.png" density="land-xhdpi" qualifier="land-xhdpi" />

        <splash src="www/res/splash.png" density="port-ldpi" qualifier="port-ldpi" />
        <splash src="www/res/splash.png" density="port-mdpi" qualifier="port-mdpi" />
        <splash src="www/res/splash.png" density="port-hdpi" qualifier="port-hdpi" />
        <splash src="www/res/splash.png" density="port-xhdpi" qualifier="port-xhdpi" />

        <allow-intent href="market:*" />
    </platform>
    
</widget>