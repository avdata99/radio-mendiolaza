<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.mendiolaza.radio" version="1.1.129" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Radio Mendiolaza</name>
    <description>
        Escuchar Radio Mendiolaza vía streaming
    </description>
    <author email="radio@data99.com.ar" href="http://radio.mendiolaza.com.ar">
        Equipo técnico de Radio Mendiolaza
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="hdpi" src="www/res/logo-64x64.png" />
    <splash density="mdpi" height="202" src="www/res/splash.png" width="202" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
