<?xml version='1.0' encoding='utf-8'?>
<widget id="ar.com.mendiolaza.radio" version="1.1.141" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <name>Radio Mendiolaza</name>
    <description>
        Escuchar Radio Mendiolaza vía streaming
    </description>
    <author email="radio@data99.com.ar" href="http://radio.mendiolaza.com.ar">
        Equipo técnico de Radio Mendiolaza
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <icon density="ldpi" qualifier="ldpi" src="/www/res/logo-64x64.png" />
    <icon density="mdpi" qualifier="mdpi" src="/www/res/logo-64x64.png" />
    <icon density="hdpi" qualifier="hdpi" src="/www/res/logo-64x64.png" />
    <icon density="xhdpi" qualifier="xhdpi" src="/www/res/logo-64x64.png" />
    <splash density="land-ldpi" qualifier="land-ldpi" src="www/res/splash.png" />
    <splash density="land-mdpi" qualifier="land-mdpi" src="www/res/splash.png" />
    <splash density="land-hdpi" qualifier="land-hdpi" src="www/res/splash.png" />
    <splash density="land-xhdpi" qualifier="land-xhdpi" src="www/res/splash.png" />
    <splash density="port-ldpi" qualifier="port-ldpi" src="www/res/splash.png" />
    <splash density="port-mdpi" qualifier="port-mdpi" src="www/res/splash.png" />
    <splash density="port-hdpi" qualifier="port-hdpi" src="www/res/splash.png" />
    <splash density="port-xhdpi" qualifier="port-xhdpi" src="www/res/splash.png" />
    <allow-intent href="market:*" />
    <preference name="loglevel" value="DEBUG" />
</widget>
